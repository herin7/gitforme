{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run GitForMe Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": {
        "type": "npm",
        "script": "compile"
      }
    }
  ]
}
